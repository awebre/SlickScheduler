@model List<SlickScheduler.Models.User>
@{
    ViewBag.Title = "AdvisorAccount";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Request An Advisor Account</h2>

@if (Request.IsAuthenticated)
{
    <p>Click the submit button to send an automatic request to the site admin for an advisor account.</p>
    <a href="@Url.Action("AdvisorAccount", new { sendRequest = true })" class="btn btn-default" type="submit" value="AdvisorAccount">Submit Request</a>
}
